    list posJupiter(integer jd, float jdf) {
        float tau = ((jd - 2451545) / 365250.0) + (jdf / 365250.0);
        float tau2 = tau * tau;
        float tau3 = tau2 * tau;
        float tau4 = tau3 * tau;
        float tau5 = tau4 * tau;

        float L = 0;
        float B = 0;
        float R = 0;

        integer i;
        integer n;
        float x;

        //  L0

        n = llGetListLength(JupiterTermL0);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermL0, i) *
                llCos(llList2Float(JupiterTermL0, i + 1) +
                      llList2Float(JupiterTermL0, i + 2) * tau);
        }
        L += x;

        //  L1

        n = llGetListLength(JupiterTermL1);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermL1, i) *
                llCos(llList2Float(JupiterTermL1, i + 1) +
                      llList2Float(JupiterTermL1, i + 2) * tau);
        }
        L += x * tau;

        //  L2

        n = llGetListLength(JupiterTermL2);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermL2, i) *
                llCos(llList2Float(JupiterTermL2, i + 1) +
                      llList2Float(JupiterTermL2, i + 2) * tau);
        }
        L += x * tau2;

        //  L3

        n = llGetListLength(JupiterTermL3);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermL3, i) *
                llCos(llList2Float(JupiterTermL3, i + 1) +
                      llList2Float(JupiterTermL3, i + 2) * tau);
        }
        L += x * tau3;

        //  L4

        n = llGetListLength(JupiterTermL4);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermL4, i) *
                llCos(llList2Float(JupiterTermL4, i + 1) +
                      llList2Float(JupiterTermL4, i + 2) * tau);
        }
        L += x * tau4;

        //  L5

        n = llGetListLength(JupiterTermL5);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermL5, i) *
                llCos(llList2Float(JupiterTermL5, i + 1) +
                      llList2Float(JupiterTermL5, i + 2) * tau);
        }
        L += x * tau5;

        //  B0

        n = llGetListLength(JupiterTermB0);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermB0, i) *
                llCos(llList2Float(JupiterTermB0, i + 1) +
                      llList2Float(JupiterTermB0, i + 2) * tau);
        }
        B += x;

        //  B1

        n = llGetListLength(JupiterTermB1);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermB1, i) *
                llCos(llList2Float(JupiterTermB1, i + 1) +
                      llList2Float(JupiterTermB1, i + 2) * tau);
        }
        B += x * tau;

        //  B2

        n = llGetListLength(JupiterTermB2);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermB2, i) *
                llCos(llList2Float(JupiterTermB2, i + 1) +
                      llList2Float(JupiterTermB2, i + 2) * tau);
        }
        B += x * tau2;

        //  B3

        n = llGetListLength(JupiterTermB3);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermB3, i) *
                llCos(llList2Float(JupiterTermB3, i + 1) +
                      llList2Float(JupiterTermB3, i + 2) * tau);
        }
        B += x * tau3;

        //  B4

        n = llGetListLength(JupiterTermB4);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermB4, i) *
                llCos(llList2Float(JupiterTermB4, i + 1) +
                      llList2Float(JupiterTermB4, i + 2) * tau);
        }
        B += x * tau4;

        //  B5

        n = llGetListLength(JupiterTermB5);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermB5, i) *
                llCos(llList2Float(JupiterTermB5, i + 1) +
                      llList2Float(JupiterTermB5, i + 2) * tau);
        }
        B += x * tau5;

        //  R0

        n = llGetListLength(JupiterTermR0);
        x = 0;
        for (i = 0; i < n; i += 3) {
            x += llList2Float(JupiterTermR0, i) *
                llCos(llList2Float(JupiterTermR0, i + 1) +
                      llList2Float(JupiterTermR0, i + 2) * tau);
        }
        R += x;

        //  R1

        n = llGetListLength(JupiterTermR1);
        x = 0;
        for (i = 0; i < n; i += 3) {
